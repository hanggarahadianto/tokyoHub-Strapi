{
  "kind": "collectionType",
  "collectionName": "stores",
  "info": {
    "singularName": "store",
    "pluralName": "stores",
    "displayName": "Store",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 20,
      "minLength": 3
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "openingHours": {
      "displayName": "Opening Hours",
      "type": "component",
      "repeatable": true,
      "component": "opening-hours.opening-hours"
    },
    "store_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::store-category.store-category",
      "inversedBy": "stores"
    },
    "phone": {
      "type": "string"
    },
    "floor": {
      "type": "string"
    }
  }
}
