{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "event_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event-category.event-category",
      "inversedBy": "events"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "short_description": {
      "type": "text",
      "required": true
    },
    "image_slide": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "location": {
      "type": "text",
      "required": true
    },
    "long_description": {
      "type": "richtext",
      "required": true
    },
    "start_end_date": {
      "type": "string",
      "required": true
    },
    "start_date": {
      "type": "date",
      "required": true
    },
    "end_date": {
      "required": true,
      "type": "date"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "shared.seo"
    }
  }
}
